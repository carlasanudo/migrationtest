{
  "name": "imageproject-backend",
  "version": "1.0.0",
  "description": "Backend API with Node.js, Express and PostgreSQL",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "init-db": "tsx scripts/initDatabase.ts",
    "migrate": "node-pg-migrate",
    "migrate:up": "tsx scripts/migrateHelper.ts up",
    "migrate:down": "tsx scripts/migrateHelper.ts down",
    "migrate:create": "tsx scripts/createMigration.ts"
  },
  "keywords": [
    "node",
    "postgresql",
    "express",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "node-pg-migrate": "^8.0.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

