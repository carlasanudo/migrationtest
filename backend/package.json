{
  "name": "imageproject-backend",
  "version": "1.0.0",
  "description": "Backend API with Node.js, Express and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/initDatabase.js",
    "migrate": "node-pg-migrate",
    "migrate:up": "node scripts/migrateHelper.js up",
    "migrate:down": "node scripts/migrateHelper.js down",
    "migrate:create": "node scripts/createMigration.js"
  },
  "keywords": [
    "node",
    "postgresql",
    "express",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "node-pg-migrate": "^8.0.3",
    "nodemon": "^3.0.1"
  }
}

